<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CInsert">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="어떤 정보를 입력하시겠습니까?">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Text>
      <Button fx:id="btnInsertWatch" mnemonicParsing="false" onAction="#insertWatch" prefHeight="40.0" prefWidth="120.0" text="관찰 정보">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="btnInsertPicture" mnemonicParsing="false" onAction="#insertPicture" prefHeight="40.0" prefWidth="120.0" text="사진">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="btnInsertSpecimen" mnemonicParsing="false" onAction="#insertSpecimen" prefHeight="40.0" prefWidth="120.0" text="표본">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="btnInsertAnticeptic" mnemonicParsing="false" onAction="#insertAnticeptic" prefHeight="40.0" prefWidth="120.0" text="방부제">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="btnInsertExit" mnemonicParsing="false" onAction="#insertExit" prefHeight="40.0" prefWidth="120.0" text="이전">
         <VBox.margin>
            <Insets bottom="10.0" top="50.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
